 <%= form_for [:admins, @user] do |f| %> <!--need :admins since namespaced-->
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

     <%= f.label :role %>
     <%= f.text_field :role %>

     <%= f.label :bio %>
     <%= f.text_field :bio %>

    <%= f.submit %>
 <% end %>